{
  "name": "@crs-k/stale-branches",
  "version": "0.0.0",
  "private": true,
  "description": "Finds and deletes stale branches",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "set NODE_OPTIONS=--openssl-legacy-provider && ncc build --source-map",
    "test": "jest --coverage",
    "all": "npm run format && npm run lint && npm test && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/crs-k/stale-branches.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0",
    "@octokit/plugin-paginate-rest": "2.17.0",
    "@octokit/types": "6.34.0",
    "@types/semver": "7.3.9",
    "assert": "2.0.0",
    "node-fetch": ">=3.1.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/parser": "5.12.1",
    "@vercel/ncc": "0.33.3",
    "eslint": "8.9.0",
    "eslint-plugin-github": "4.3.5",
    "eslint-plugin-jest": "26.1.1",
    "js-yaml": "4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
