{
  "name": "@crs-k/stale-branches",
  "version": "0.0.0",
  "private": true,
  "author": "Chris Kerins",
  "license": "MIT",
  "description": "Finds and deletes stale branches",
  "main": "lib/main.js",
  "homepage": "https://github.com/marketplace/actions/stale-branches",
  "bugs": {
    "url": "https://github.com/crs-k/stale-branches/issues"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "npm run npmpackagejsonlint . && npm run eslint",
    "eslint": "eslint src/**/*.ts",
    "npmpackagejsonlint": "npmPkgJsonLint .",
    "package": "set NODE_OPTIONS=--openssl-legacy-provider && ncc build --source-map",
    "test": "jest --coverage",
    "all": "npm run format && npm run lint && npm test && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/crs-k/stale-branches.git"
  },
  "keywords": [
    "actions"
  ],
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.0",
    "@octokit/plugin-paginate-rest": "13.1.0",
    "@octokit/types": "12.5.0",
    "@types/semver": "7.7.0",
    "assert": "2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.30.0",
    "@octokit/rest": "22.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "24.0.10",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.1",
    "@vercel/ncc": "0.38.3",
    "ansi-styles": "5.2.0",
    "eslint": "9.29.0",
    "eslint-plugin-github": "5.1.8",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-prettier": "5.5.1",
    "globals": "16.2.0",
    "js-yaml": "4.1.0",
    "npm-package-json-lint": "8.0.0",
    "prettier": "3.6.1",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
